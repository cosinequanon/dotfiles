[user]
    email = sgoder@fb.com
    name = cosinequanon
[core]
    editor = vim
    whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
    excludesfile = ~/.gitignore
[alias]
    cl = clone
    cm = commit
    cmm = commit -m
    cma = commit --amend
    co = checkout
    br = branch
    st = status
    di = diff
    f = fetch
    fo = fetch origin
    pl = pull
    plo = pull origin
    plr = pull --rebase
    ps = push
    pso = push origin
    psf = push --force
    rb = rebase
